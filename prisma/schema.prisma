// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {    
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Rawmaterial {
  id    Int     @id @default(autoincrement())
  name  String
  price Float
}

model Box {
  id Int @id @default(autoincrement())
  name String
  price Float
}

model Client {
  id Int @id @default(autoincrement())
  name String
  contract String
  country String
}

model Freight {
    id Int @id @default(autoincrement())
    name String
    destination String
    weight Float
    price Float
}